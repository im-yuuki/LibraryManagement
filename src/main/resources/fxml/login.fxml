<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="320.0"
      spacing="20.0" xmlns="http://javafx.com/javafx/21.0.5" xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
   </padding>
   <HBox alignment="CENTER" prefWidth="200.0">
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="ID Number"/>
      <Region prefHeight="0.0" HBox.hgrow="ALWAYS"/>
      <TextField fx:id="idField" prefWidth="180.0" promptText="ID"/>
   </HBox>
   <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefWidth="200.0">
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password"/>
      <Region prefHeight="0.0" HBox.hgrow="ALWAYS"/>
      <PasswordField fx:id="passwordField" prefWidth="180.0" promptText="Password"/>
   </HBox>
   <HBox alignment="TOP_RIGHT" prefWidth="200.0" spacing="10.0">
      <Button mnemonicParsing="false" onAction="#onSubmit" text="Login"/>
      <Button mnemonicParsing="false" onAction="#onCancel" text="Exit"/>
   </HBox>
</VBox>
